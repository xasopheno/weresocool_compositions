{ f: 311.127, l: 1, g: 1/3, p: 0 }

thing1(a, b) = {
    Overlay [
        {1/1, 2, 1, 1},
        {1/1, 0, 1, -1},
    ]
    | Seq [
        a, b, a
    ]
}

thing2(b) = {
    Overlay [
        {1/1, 3, 1, 1},
        {1/1, 0, 1, -1},
    ]
    | Seq [
        thing1(thing1(b, b))
    ]
    | FitLength thing1
}

main = {
    Overlay [
        thing1(thing2(Fm 3/4), thing2(Fm 5/4)),
    ]
}
